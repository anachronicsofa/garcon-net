<div class="btn_container" id=<%=command.id%> >
  <% unless command.paid?%>
    <%= link_to 'Confirmar Pagamento', command_confirm_payment_path({id: command.id}), remote: true, method: :post, class: "btn btn--link" %>
  <% else %>
    <%= link_to 'Pagamento Confirmado!', order_path(command.order), class: "btn btn--show", disable: true %>
  <% end %>
</div>